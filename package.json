{
  "name": "real-estate-cms-backend",
  "version": "1.0.0",
  "description": "Headless Real Estate CMS Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:https": "ts-node-dev --respawn --transpile-only src/https-server.ts",
    "generate-certs": "node generate-certificates.js",
    "generate-certs:windows": "node generate-certificates-windows.js",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:https": "node dist/https-server.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:migrate:status": "npx sequelize-cli db:migrate:status",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed",
    "db:fresh": "npm run db:migrate:undo:all && npm run db:migrate"
  },
  "keywords": [
    "real-estate",
    "cms",
    "api",
    "headless"
  ],
  "author": "Real Estate CMS Team",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.3.3",
    "node-forge": "^1.3.1",
    "redis": "^4.6.7",
    "sequelize": "^6.32.1",
    "sharp": "^0.32.6",
    "slugify": "^1.6.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.3.1",
    "@types/uuid": "^9.0.2",
    "sequelize-cli": "^6.6.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3"
  }
}
